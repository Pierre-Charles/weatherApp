{
  "presets": [
    "@babel/preset-env", 
    "@babel/preset-react"
  ],
  "plugins": [
      [
        "@babel/transform-runtime", 
        {
          "absoluteRuntime": false,
          "corejs": false,
          "helpers": true,
          "regenerator": true,
          "useESModules": false
        }
      ],
      [
        "@babel/plugin-proposal-class-properties", 
        { 
          "loose": true 
        }
      ]
  ]
}